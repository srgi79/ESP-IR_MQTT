#define IR_LED 4  // ESP8266 GPIO pin to use. Recommended: 4 (D2).
#define IR_LED 5  // ESP8266 GPIO pin to use. Recommended: 4 (D1).

IRsend irsend(IR_LED);  // Set the GPIO to be used to sending the message.

void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  digitalWrite(LED_BUILTIN, HIGH);
  irsend.begin();
}

void loop() {
  digitalWrite(LED_BUILTIN, LOW);
  irsend.sendGC(powerRaw, 135);
  digitalWrite(LED_BUILTIN, HIGH);
  delay(3000);
  irsend.sendGC(sourceRaw, 135);
  delay(3000);
  irsend.sendGC(volUpRaw, 135);
  delay(3000);
  irsend.sendGC(volDwnRaw, 135);
  delay(3000);
}
